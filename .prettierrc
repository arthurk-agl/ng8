{
    "printWidth": 140,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.html",
            "options": {}
        },
        {
            "files": "*.scss",
            "options": {}
        },
        {
            "files": "*.ts",
            "options": {}
        },
        {
            "files": "*.json",
            "options": {
                "singleQuote": false
            }
        }
    ]
}
